<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="anchorPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
            minHeight="-Infinity" minWidth="-Infinity" prefHeight="900.0" prefWidth="1600.0"
            style="-fx-background-color: grey;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="FrontPageController">
    <children>
        <HBox alignment="CENTER_RIGHT" fillHeight="false" maxWidth="200.0" prefHeight="50.0" prefWidth="200.0" spacing="10.0" style="-fx-background-color: #505050;" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
            <children>
                <Hyperlink fx:id="CreateProfileHyperlink" onAction="#pressCreateProfile" text="Create Profile" textFill="WHITE" textOverrun="WORD_ELLIPSIS">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Hyperlink>
                <Hyperlink fx:id="SignInHyperlink" onAction="#pressSignIn" text="Sign In" textFill="WHITE" textOverrun="WORD_ELLIPSIS">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Hyperlink>
                <MenuButton fx:id="ProfileMenuButton" mnemonicParsing="false" onAction="#chooseViewProfile" text="Profile">
                    <items>
                        <MenuItem mnemonicParsing="false" text="View Profile" />
                        <MenuItem mnemonicParsing="false" text="Edit Profile" />
                        <MenuItem mnemonicParsing="false" text="Your Groups" />
                    </items>
                    <graphic>
                        <Button mnemonicParsing="false">
                            <graphic>
                                <ImageView fitHeight="27.0" fitWidth="23.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../ImageView/Icons/profile_icon.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                    </graphic>
                </MenuButton>
            </children>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <padding>
                <Insets right="20.0" />
            </padding>
        </HBox>
        <HBox alignment="CENTER_LEFT" prefHeight="50.0" spacing="10.0" style="-fx-background-color: #505050;" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">
            <children>
                <TextField alignment="TOP_LEFT" prefHeight="20.0" prefWidth="400.0" promptText="Search Destination" />
            </children>
            <padding>
                <Insets left="20.0" />
            </padding>
        </HBox>
        <HBox layoutY="65.0" prefHeight="70.0" prefWidth="1920.0" spacing="50.0" style="-fx-background-color: #303030;" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="70.0">
            <children>
                <Label alignment="TOP_LEFT" text="A400b" textFill="#545454">
                    <font>
                        <Font name="System Bold" size="48.0" />
                    </font>
                </Label>
            </children>
            <padding>
                <Insets left="20.0" />
            </padding>
        </HBox>
        <VBox layoutX="199.0" layoutY="301.0" prefHeight="1030.0" prefWidth="250.0" style="-fx-background-color: #303030;" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="150.0">
            <children>
                <Button maxWidth="250.0" mnemonicParsing="false" onAction="#pressAllDestinations" prefHeight="100.0" style="-fx-background-color: #505050;" text="All Destinations" textFill="WHITE" />
                <Button maxWidth="250.0" mnemonicParsing="false" style="-fx-background-color: #303030;" text="Button" textFill="WHITE">
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </Button>
                <Button maxWidth="250.0" mnemonicParsing="false" style="-fx-background-color: #303030;" text="Button" textFill="WHITE">
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </Button>
                <Button maxWidth="250.0" mnemonicParsing="false" style="-fx-background-color: #303030;" text="Button" textFill="WHITE">
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
        <Button layoutX="275.0" layoutY="188.0" mnemonicParsing="false" onAction="#chooseViewProfile" text="goto test" />
    </children>
</AnchorPane>
