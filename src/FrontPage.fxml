<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="1080.0"
            prefWidth="1920.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="FrontPageController">
    <children>
        <HBox alignment="TOP_RIGHT" fillHeight="false" prefHeight="100.0" prefWidth="960.0" spacing="10.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Hyperlink fx:id="CreateProfileHyperlink" onAction="#pressCreateProfile" text="Create Profile"/>
                <Hyperlink fx:id="SignInHyperlink" onAction="#pressSignIn" text="Sign In"/>
                <SplitMenuButton fx:id="ProfileButton" mnemonicParsing="false" text="Profile">
                    <items>
                        <MenuItem mnemonicParsing="false" text="View Profile"/>
                        <MenuItem mnemonicParsing="false" text="Edit Profile"/>
                        <MenuItem mnemonicParsing="false" text="Your Groups"/>
                    </items>
                </SplitMenuButton>
            </children>
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
            <padding>
                <Insets right="20.0" top="20.0"/>
            </padding>
        </HBox>
        <HBox prefHeight="0.0" prefWidth="960.0" spacing="10.0" AnchorPane.leftAnchor="20.0"
              AnchorPane.topAnchor="20.0">
            <children>
                <TextField alignment="TOP_LEFT" prefHeight="20.0" prefWidth="400.0" promptText="Search Destination"/>
            </children>
        </HBox>
    </children>
</AnchorPane>
