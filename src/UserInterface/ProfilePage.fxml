            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
        <TextField fx:id="searchField" layoutX="436.0" layoutY="494.0" prefHeight="27.0" prefWidth="417.0"/>
        <Text layoutX="436.0" layoutY="473.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="SEARCH FIELD FOR DESTINATIONS"/>
        <Label layoutX="436.0" layoutY="45.0" text="Basic Details">
            <font>
                <Font size="48.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
