<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UserInterface.ProfilePageController">
   <children>
      <HBox>
         <children>
            <Button minWidth="200.0" mnemonicParsing="false" text="Your Profile" />
            <Button minWidth="200.0" mnemonicParsing="false" text="Edit Profile" />
            <Button minWidth="200.0" mnemonicParsing="false" text="Your Groups" />
            <Button minWidth="200.0" mnemonicParsing="false" text="Add Information" />
         </children>
      </HBox>
      <VBox />
      <Label layoutX="14.0" layoutY="34.0" text="Welcome">
         <font>
            <Font name="Arial Bold" size="20.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel" layoutX="110.0" layoutY="37.0">
         <font>
            <Font name="Arial Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="FrontPageButton" layoutX="210.0" layoutY="33.0" mnemonicParsing="false" onAction="#pressFrontPageButton" text="Front Page" />
   </children>
</AnchorPane>
