            <children>
                <Label layoutX="41.0" layoutY="39.0" text="Welcome Back" textFill="WHITE">
                    <font>
                        <Font name="Arial Bold" size="36.0"/>
                    </font>
                </Label>
                <VBox alignment="TOP_CENTER" layoutY="466.0" maxWidth="-Infinity" minWidth="-Infinity"
                      prefHeight="450.0" prefWidth="350.0" spacing="25.0" style="-fx-background-color: green;"
                      AnchorPane.bottomAnchor="0.0">
                    <children>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <ImageView fitHeight="53.0" fitWidth="53.0" pickOnBounds="true">
                                    <effect>
                                        <ColorAdjust brightness="1.0"/>
                                    </effect>
                                    <image>
                                        <Image url="@../ImageView/Icons/edit_icon.png"/>
                                    </image>
                                </ImageView>
                                <Button mnemonicParsing="false" style="-fx-background-color: green;" text="Edit Profile"
                                        textFill="WHITE">
                                    <font>
                                        <Font name="System Bold" size="18.0"/>
                                    </font>
                                </Button>
                            </children>
                        </HBox>
                    </children>
                </VBox>
                <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="100.0" layoutY="96.0" pickOnBounds="true"
                           preserveRatio="true">
                    <effect>
                        <ColorAdjust brightness="1.0" hue="-0.02" saturation="1.0"/>
                    </effect>
                    <image>
                        <Image url="@../ImageView/Icons/profile_icon2.png"/>
                    </image>
                </ImageView>
                <HBox alignment="CENTER" layoutY="231.0" prefHeight="100.0" prefWidth="350.0"
                      AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                    <children>
                        <Label fx:id="usernameLabel" text="UsernameLoggedIn" textAlignment="CENTER" textFill="WHITE">
                            <font>
                                <Font name="Helonia" size="24.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
                <Button layoutX="143.0" layoutY="438.0" mnemonicParsing="false" onAction="#openGroupCreation"
                        text="Button"/>
            </children>
        </AnchorPane>
        <GridPane layoutX="439.0" layoutY="144.0" prefHeight="257.0" prefWidth="408.0">
            <children>
                <Label text="First Name">
                    <font>
                        <Font size="30.0"/>
                    </font>
                </Label>
                <Label text="Last Name" GridPane.rowIndex="1">
                    <font>
                        <Font size="30.0"/>
                    </font>
                </Label>
                <Label text="Gender" GridPane.rowIndex="2">
                    <font>
                        <Font size="30.0"/>
                    </font>
                </Label>
                <Label text="Age" GridPane.rowIndex="3">
                    <font>
                        <Font size="30.0"/>
                    </font>
                </Label>
                <Label fx:id="firstnameLabel" text="Label" GridPane.columnIndex="1">
                    <font>
                        <Font size="24.0"/>
                    </font>
                </Label>
                <Label fx:id="lastnameLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <font>
                        <Font size="24.0"/>
                    </font>
                </Label>
                <Label fx:id="genderLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <font>
                        <Font size="24.0"/>
                    </font>
                </Label>
                <Label fx:id="ageLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3">
                    <font>
                        <Font size="24.0"/>
                    </font>
                </Label>
            </children>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
